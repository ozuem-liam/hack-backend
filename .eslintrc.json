{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
      },
    "extends": ["airbnb", "prettier", "eslint:recommended", "plugin:node/recommended"],
    "parserOptions": {
        "ecmaVersion": 11
      },
    "plugins": ["prettier"],
    "rules": {
        "indent": ["error", "tab"],
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-useless-catch": "off",
        "prettier/prettier": ["error"]
    }
}